x = input().split()
a = int(x[0])
b = int(x[1])
c = int(x[2])

#make list
list = [a, b, c]
#sort the list.
list.sort()
#print the sorted list
print(list[0])
print(list[1])
print(list[2])
print()
# print the values as these readed
print(a)
print(b)
print(c)
