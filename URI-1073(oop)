# >>------------------uri online judge-----------------------------------------------<<
# >>-----------------Even Square----------------------------------------------------<<

class Even:
    def __init__(self,n):
        self.n=n
    def even(self):
        for self.i in range(1,(self.n+1)):
            if (self.i%2)==0:
                print("%d^2 = %d"%(self.i,self.i**2))
n=int(input())
obj=Even(n)
obj.even()
